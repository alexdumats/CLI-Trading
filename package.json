{
  "name": "claude-multi-agent-trader-root",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "verify": "npm run format:check",
    "health:check": "node scripts/health_check.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --config=jest.config.js",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --config=jest.config.js --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --config=jest.config.js --coverage",
    "test:unit": "NODE_OPTIONS=--experimental-vm-modules jest --config=jest.config.js --testPathPattern=tests/unit/",
    "test:integration": "NODE_OPTIONS=--experimental-vm-modules jest --config=jest.config.js --testPathPattern=tests/integration/",
    "test:integration:docker": "RUN_DOCKER_TESTS=true NODE_OPTIONS=--experimental-vm-modules jest --config=jest.config.js --testPathPattern=tests/integration-jest/",
    "test:e2e": "NODE_OPTIONS=--experimental-vm-modules jest --config=jest.config.js --testPathPattern=tests/e2e/"
  },
  "devDependencies": {
    "axios": "^1.7.7",
    "express": "^4.19.2",
    "husky": "^9.0.11",
    "ioredis": "^5.4.1",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.5",
    "pg": "^8.12.0",
    "prettier": "^3.3.3",
    "prom-client": "^15.1.3",
    "sinon": "^17.0.1",
    "supertest": "^6.3.4"
  },
  "lint-staged": {
    "*.{js,jsx,json,md,yml,yaml}": [
      "prettier -w"
    ]
  }
}
